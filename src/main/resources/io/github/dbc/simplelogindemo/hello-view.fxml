<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="431.0" prefWidth="635.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="io.github.dbc.simplelogindemo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="usernameField" promptText="username" />
   <PasswordField fx:id="passwordField" promptText="password" />
   <Button fx:id="loginButton" mnemonicParsing="false" text="Log In" onMouseClicked="#logIn"/>
</VBox>
